<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="searchVM" type="com.adityasapplication.app.modules.search.data.viewmodel.SearchVM"/>
    </data>
    <FrameLayout android:id="@+id/frameSearch"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_900">
        <LinearLayout android:id="@+id/linearColumnmenu"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowmenu"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageMenu"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="@dimen/_35pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_menu"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_menu"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtKYC"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_101pxh"
                        android:layout_weight="0.65"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{searchVM.searchModel.txtKYC}"
                        style="@style/txtMontserratromansemibold32" tools:text="@string/lbl_kyc"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <androidx.appcompat.widget.SearchView
                android:background="@drawable/rectangle_bg_bluegray_900_radius_22"
                android:gravity="center_horizontal"
                android:id="@+id/searchViewSearchBar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="match_parent"
                android:theme="@style/searchViewSearchBar"
                app:iconifiedByDefault="false"
                app:queryBackground="@android:color/transparent"
                app:queryHint="@string/msg_search_for_anyt" app:searchIcon="@drawable/img_search_gray_601"/>
            <FrameLayout android:id="@+id/frameStackuser"
                android:layout_height="@dimen/_304pxv"
                android:layout_marginTop="@dimen/_424pxv" android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearColumnhome"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_22pxv"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout android:id="@+id/linearRowsearch"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <FrameLayout
                            android:id="@+id/frameStacktelevision"
                            android:layout_height="@dimen/_25pxv"
                            android:layout_marginStart="@dimen/_1pxh" android:layout_width="@dimen/_271pxh">
                            <LinearLayout
                                android:id="@+id/linearRowmaterialsymbol"
                                android:layout_gravity="bottom|start"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:layout_width="match_parent" android:orientation="horizontal">
                                <LinearLayout
                                    android:id="@+id/linearRowhome"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_2pxv"
                                    android:layout_marginTop="@dimen/_1pxv"
                                    android:layout_weight="0.82"
                                    android:layout_width="0dp" android:orientation="horizontal">
                                    <FrameLayout
                                    android:id="@+id/frameStackhome"
                                    android:layout_height="@dimen/_20pxv"
                                    android:layout_marginBottom="@dimen/_1pxv" android:layout_width="@dimen/_21pxh">
                                    <ImageView
                                    android:id="@+id/imageHome"
                                    android:layout_gravity="start"
                                    android:layout_height="@dimen/_20pxh"
                                    android:layout_width="@dimen/_21pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_home"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                                    <ImageView
                                    android:id="@+id/imageHomeOne"
                                    android:layout_gravity="start"
                                    android:layout_height="@dimen/_20pxh"
                                    android:layout_width="@dimen/_21pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_home"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                                    </FrameLayout>
                                    <ImageView
                                    android:id="@+id/imageTelevision"
                                    android:layout_height="@dimen/_20pxh"
                                    android:layout_marginStart="@dimen/_59pxh"
                                    android:layout_marginTop="@dimen/_1pxv"
                                    android:layout_width="@dimen/_20pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_television"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_television"/>
                                </LinearLayout>
                                <ImageView
                                    android:id="@+id/imageMaterialsymbol"
                                    android:layout_height="@dimen/_19pxh"
                                    android:layout_marginBottom="@dimen/_4pxv"
                                    android:layout_marginEnd="@dimen/_1pxh"
                                    android:layout_marginStart="@dimen/_147pxh"
                                    android:layout_marginTop="@dimen/_1pxv"
                                    android:layout_width="@dimen/_22pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_materialsymbol"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_materialsymbol"/>
                            </LinearLayout>
                            <ImageView android:id="@+id/imageSearch"
                                android:layout_gravity="top|end"
                                android:layout_height="@dimen/_23pxh"
                                android:layout_marginBottom="@dimen/_10pxv"
                                android:layout_marginEnd="@dimen/_90pxh"
                                android:layout_marginStart="@dimen/_90pxh"
                                android:layout_width="@dimen/_18pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_search"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
                        </FrameLayout>
                        <ImageView android:id="@+id/imageUser"
                            android:layout_height="@dimen/_18pxh"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_marginStart="@dimen/_47pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_user"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowhomeTwo"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtHomeTwo"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.19"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{searchVM.searchModel.txtHomeTwo}"
                            style="@style/txtMontserratromanlight6" tools:text="@string/lbl_home"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDetails"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:layout_marginStart="@dimen/_60pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.21"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{searchVM.searchModel.txtDetails}"
                            style="@style/txtMontserratromanlight6" tools:text="@string/lbl_details"/>
                        <TextView android:id="@+id/txtCollegePredict"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_56pxh"
                            android:layout_weight="0.28"
                            android:layout_width="0dp"
                            android:text="@{searchVM.searchModel.txtCollegePredict}"
                            style="@style/txtMontserratromanlight6" tools:text="@string/msg_college_predict"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLive"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginStart="@dimen/_67pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_weight="0.12"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{searchVM.searchModel.txtLive}"
                            style="@style/txtMontserratromanlight6" tools:text="@string/lbl_live"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtProfile"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginStart="@dimen/_53pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_weight="0.19"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{searchVM.searchModel.txtProfile}"
                            style="@style/txtMontserratromanlight6" tools:text="@string/lbl_profile"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearColumnkeyboard"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylebluegray_100">
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtKeyBoard"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_174pxv"
                        android:layout_marginEnd="@dimen/_116pxh"
                        android:layout_marginStart="@dimen/_116pxh"
                        android:layout_marginTop="@dimen/_89pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{searchVM.searchModel.txtKeyBoard}"
                        style="@style/txtMontserratromanregular32" tools:text="@string/lbl_keyboard"/>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
